#include <stdio.h>
int main() {
int x = 5; //Basic
int y = 3;
//Assignment
printf("%d\n", x + y); 
//Relational and Logical
printf("%d", x > 3 && y < 10); 
}

#include <stdio.h>
int main() {
int myNum = 15; // Integer (whole number)
float myFloatNum = 5.99; // Floating point number(default show 6 places after drcimal
char myLetter = 'D'; // Character
printf("%d\n", myNum);
printf("%f\n", myFloatNum);
printf("%c\n", myLetter);}

